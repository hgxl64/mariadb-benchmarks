#
# MariaDB specific
#

# Marker
export IS_MARIADB=1
[[ ${IS_MYSQL} ]] && unset IS_MYSQL

# Command Line Tools
config_variable MYSQL            mariadb
config_variable MYSQLD           mariadbd
config_variable MYSQLADMIN       mariadb-admin
config_variable MYSQLD_SAFE      mariadbd-safe
config_variable MYSQL_INSTALL_DB mariadb-install-db


