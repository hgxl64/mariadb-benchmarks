[mysqld]

#general settings
secure_file_priv = /tmp

#MySQL compatibility
loose_default_authentication_plugin=mysql_native_password
skip_log_bin

performance_schema = false

# MyISAM engine
key_buffer_size = 1G
key_cache_segments = 16

# Higher Concurrency
max_prepared_stmt_count = 1048576
max_connections = 1000
table_open_cache = 2000

